{
  "name": "neural-activity-visualizer-react",
  "version": "1.0.2",
  "description": "A ReactJS component for visualizing neural activity data",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/index.js",
  "dependencies": {
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "build-lib": "rollup -c",
    "build-lib-watch": "rollup -c -w",
    "build-demo": "npm run build-lib && cd demo && react-scripts build",
    "start-demo": "cd demo && npm run start",
    "i-all": "npm i && cd demo && npm i",
    "dev": "npm-run-all --parallel build-lib-watch start-demo"
  },
  "keywords": [],
  "author": "Andrew Davison <andrew.davison@cnrs.fr>, CNRS",
  "license": "MIT",
  "homepage": "https://github.com/NeuralEnsemble/neo-viewer/blob/master/js/react#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/NeuralEnsemble/neo-viewer.git",
    "directory": "js/react"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@rollup/plugin-babel": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.26.4",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.3"
  },
  "peerDependencies": {
    "axios": "^0.24.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "plotly.js": "^1.58.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-plotly.js": "^2.5.1"
  }
}